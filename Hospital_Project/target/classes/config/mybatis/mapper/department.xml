<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dept">
 <select id="dept_list" resultType="dept">
    SELECT 
        dept_category, 
        dept_name
    FROM 
        department
    ORDER BY 
        dept_category, 
        dept_name
</select>


</mapper>